{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "opener:default",
    "dialog:default",
    "dialog:allow-open",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-app-write",
    "fs:allow-app-write-recursive",
    "shell:default",
    "store:default",
    "http:default",
    "http:allow-fetch",
    "http:allow-fetch-cancel"
  ]
}
